---
pageable: true
layout: blog_layout
---
<h2> Articles tagged '<%= tagname %>'</h2>

<% if paginate && num_pages > 1 %>
  <p>Page <%= page_number %> of <%= num_pages %></p>
<% end %>



<%= partial( :"/partials/blog_card", :locals => {}) %>


 <% if prev_page %>
      <p><%= link_to "prev" , prev_page %></p>
 <% end %>

<% if paginate %>
  <% if next_page %>
    <p><%= link_to 'Next page', next_page %></p>
  <% end %>
<% end %>
